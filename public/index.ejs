<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
    <div id="app">
        <button @click="gotolist()">查看笑话</button>
        <ul>
            <li v-for="(key,value) in list" style="margin-top: 10px">
                {{value}}
            </li>
        </ul>
    </div>

</body>
<script src="/lib/jquery.js"></script>
<script src="/lib/vue.js"></script>
<script>
    $(document).ready(function () {
        var vm = new Vue({
            el:'#app',
            data:{
                list:{}
            },
            methods:{
                gotolist:function () {
                    var _this = this
                    $.ajax({
                        url:'/query',
                        type:'get',
                        success:function (data) {
                            _this.list = data
                        }
                    })
                }
            }
        })
    })

</script>
</html>